{
  "name": "sprint-preview",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"prism mock api/openapi.yaml --port 4010 --host 0.0.0.0\" \"vite\"",
    "dev:vite": "vite",
    "dev:prism": "prism mock api/openapi.yaml --port 4010 --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "generate:types": "openapi-typescript api/openapi.yaml -o src/api/schema.d.ts"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@stoplight/prism-cli": "^5.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.0.0",
    "openapi-typescript": "^7.0.0",
    "typescript": "~5.7.0",
    "vite": "^6.0.0"
  }
}
